        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">@dimen/RowHeight</item>
        <item name="android:background">@color/whiteColor</item>
        <item name="android:paddingLeft">@dimen/uiContentPaddingLeft</item>
        <item name="android:paddingRight">@dimen/uiContentPaddingRight</item>
        <item name="com.fcloud.licai.miaodao:uiContentClickFlag">true</item>
        <item name="com.fcloud.licai.miaodao:uiContentInputFlag">false</item>
        <item name="com.fcloud.licai.miaodao:uiContentcolorFlag">false</item>
        <item name="com.fcloud.licai.miaodao:uiContentTitleColor">@color/loan_detail_text</item>
        <item name="com.fcloud.licai.miaodao:uiContentColor">@color/loan_bank_text</item>
        <item name="com.fcloud.licai.miaodao:uiContentTitleWidth">@dimen/d40</item>
        <item name="com.fcloud.licai.miaodao:uiContentTitleTextSize">@dimen/s14</item>
        <item name="com.fcloud.licai.miaodao:uiContentTextSize">@dimen/s13</item>
        <item name="com.fcloud.licai.miaodao:uiContentWidth">@dimen/uiContentContentWidth</item>
    </style>

    <style name="loanButtonStyle">
        <item name="android:background">@drawable/btn_shape_backgroup_color</item>
        <item name="android:layout_marginBottom">@dimen/buttonMarginBottom</item>
        <item name="android:layout_marginLeft">@dimen/buttonMarginLeft</item>
        <item name="android:padding">@dimen/buttonPadding_S</item>
        <item name="android:textColor">@color/whiteColor</item>
        <item name="android:layout_marginRight">@dimen/buttonMarginRight</item>
        <item name="android:layout_marginTop">@dimen/d10</item>
        <item name="android:minHeight">@dimen/buttonMinHeight</item>
        <item name="android:textSize">@dimen/contentTextSize_x</item>
        <item name="android:lines">1</item>
        <item name="android:gravity">center</item>
    </style>


    <style name="shareButtonStyle">
        <item name="android:background">@drawable/btn_shape_backgroup_color</item>
        <item name="android:layout_marginBottom">@dimen/d10</item>
        <item name="android:padding">@dimen/buttonPadding_S</item>
        <item name="android:textColor">@color/whiteColor</item>
        <item name="android:layout_marginTop">@dimen/d20</item>
        <item name="android:minHeight">@dimen/d30</item>
        <item name="android:textSize">@dimen/contentTextSize_x</item>
        <item name="android:lines">1</item>
        <item name="android:gravity">center</item>
    </style>

    <!-- 自定义loading view -->
    <style name="AppTheme.Dialog" parent="android:Theme.Light">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowBackground">@color/transparentColor</item>
        <item name="android:maxWidth">600dp</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
    </style>

    <style name="MyDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <!-- 窗口背景色 -->
        <item name="android:windowFrame">@null</item>
        <!-- Dialog的windowFrame框为无 -->
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsTranslucent">true</item>
        <!-- 窗口是否半透明——是（与第一条配合使用） -->
        <item name="android:windowContentOverlay">@null</item>
        <!-- 这个不设置的话，可能会出现边框黑线 -->
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <!-- 窗口弹出效果 -->
        <item name="android:backgroundDimEnabled">true</item>
        <!-- 是否允许背景模糊 -->
    </style>
    <!-- 自定义loading view end -->

    <!--全屏dialog 用于查看协议 背景透明-->
    <!--<item name="android:windowFrame">@null</item>&lt;!&ndash;边框&ndash;&gt;-->
    <!---->
    <style name="share_dialog">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <!--是否浮现在activity之上-->
        <item name="android:windowIsTranslucent">false</item>
        <!--半透明-->
        <item name="android:windowBackground">@color/transparentColor</item>
        <!--背景透明-->
        <item name="android:backgroundDimEnabled">true</item>
        <!--模糊-->
    </style>


    <style name="btn_verify">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">@dimen/d30</item>
        <item name="android:paddingLeft">@dimen/d5</item>
        <item name="android:paddingRight">@dimen/d5</item>
        <item name="android:layout_marginRight">@dimen/d20</item>
        <item name="android:background">@drawable/btn_shape_backgroup_color</item>
        <item name="android:text">@string/reg_getCode</item>
        <item name="android:textColor">@color/whiteColor</item>
        <item name="android:textSize">@dimen/s12</item>
        <item name="android:layout_gravity">center_vertical</item>
    </style>


    <style name="tongyi" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/tongyi</item>
        <item name="android:paddingLeft">@dimen/d5</item>
        <item name="android:maxHeight">@dimen/d15</item>
    </style>


    <style name="repay_popwindow">
        <item name="android:windowEnterAnimation">@anim/push_bottom_in</item>
        <!-- 指定显示的动画xml -->

        <item name="android:windowExitAnimation">@anim/push_bottom_out</item>
        <!-- 指定消失的动画xml -->
    </style>

